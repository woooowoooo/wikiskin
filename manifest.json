{
	"manifest_version": 2,
	"name": "Wikipedia Skinner",
	"version": "1.0",
	"description": "Choose a Wikipedia project skin without logging in!",
	"icons": {
		"48": "icons/wikiskin-48.png"
		"96": "icons/wikiskin-96.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["*://*.wikipedia.org/*"],
		"js": ["wikiskin.js"]
	}]
}