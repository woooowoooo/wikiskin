{
	"manifest_version": 2,
	"name": "Wikiskin",
	"version": "1.0",
	"description": "Choose a Wikipedia skin without logging in!",
	"icons": {
		"48": "icons/wikiskin-48.png",
		"96": "icons/wikiskin-96.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "icons/wikiskin-48.png"
		},
		"default_title": "Wikiskin",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["*://*.wikipedia.org/*"],
		"js": ["wikiskin.js"]
	}]
}